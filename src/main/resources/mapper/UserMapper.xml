<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kangaroohy.ssm_shiro.mapper.UserMapper">
    <select id="selectByUsername" parameterType="string" resultType="com.kangaroohy.ssm_shiro.domain.entity.po.User">
        SELECT * FROM t_user WHERE username = #{username}
    </select>
</mapper>
